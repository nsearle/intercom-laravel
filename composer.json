{
  "name": "nsearle/intercom-laravel",
  "description": "Laravel 5.x wrapper for Intercom-PHP",
  "keywords": [
    "intercom",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nick Searle",
      "email": "nick@weav.com"
    }
  ],
  "require": {
    "intercom/intercom-php": "4.4.2",
    "laravel/framework": ">=5.0"
  },
  "autoload": {
    "psr-4": {
      "Shadow\\IntercomLaravel\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Shadow\\IntercomLaravel\\ServiceProvider"
      ],
      "aliases": {
        "Intercom": "Shadow\\IntercomLaravel\\Facade"
      }
    }
  }
}
